{
  "format_version": "1.21.90",
  "minecraft:entity": {
    "description": {
      "identifier": "sao:dire_wolf",
      "runtime_identifier": "minecraft:wolf",
      "spawn_category": "creature",
      "is_spawnable": true,
      "is_summonable": true,
      "properties": {
        "sao:exp": {
          "type": "int",
          "range": [
            0,
            24
          ],
          "default": 24
        },
        "sao:col": {
          "type": "int",
          "range": [
            0,
            50
          ],
          "default": 50
        },
        "sao:health_bar": {
          "type": "bool",
          "client_sync": true,
          "default": "q.had_component_group('sao:health_bar_timer')"
        }
      }
    },
    "component_groups": {
      "sao:health_bar_timer": {
        "minecraft:timer": {
          "time": 5,
          "looping": false,
          "time_down_event": {
            "event": "sao:health_bar_hide"
          }
        }
      }
    },
    "components": {
      "minecraft:health": {
        "value": 400,
        "max": 400
      },
      "minecraft:experience_reward": {
        "on_death": "query.last_hit_by_player ? 24 : 0"
      },
      "minecraft:loot": {
        "table": "loot_tables/dire_wolf.json"
      },
      "minecraft:movement": {
        "value": 0.3
      },
      "minecraft:attack": {
        "damage": 4
      },
      "minecraft:type_family": {
        "family": [
          "wolf",
          "mob",
          "sao"
        ]
      },
      "minecraft:scale": {
        "value": 1.5
      },
      "minecraft:collision_box": {
        "width": 0.3,
        "height": 0.8
      },
      "minecraft:is_hidden_when_invisible": {},
      "minecraft:breathable": {
        "total_supply": 15,
        "suffocate_time": 0
      },
      "minecraft:nameable": {},
      "minecraft:hurt_on_condition": {
        "damage_conditions": [
          {
            "filters": {
              "test": "in_lava",
              "subject": "self"
            },
            "cause": "lava",
            "damage_per_tick": 4
          }
        ]
      },
      "minecraft:navigation.walk": {
        "can_path_over_water": true,
        "avoid_damage_blocks": true
      },
      "minecraft:movement.basic": {},
      "minecraft:jump.static": {},
      "minecraft:can_climb": {},
      "minecraft:balloonable": {
        "mass": 0.8
      },
      "minecraft:despawn": {
        "despawn_from_distance": {}
      },
      "minecraft:physics": {},
      "minecraft:pushable": {
        "is_pushable": true,
        "is_pushable_by_piston": true
      },
      "minecraft:conditional_bandwidth_optimization": {},
      "minecraft:damage_sensor": {
        "triggers": [
          {
            "on_damage": {
              "event": "sao:health_bar_show"
            }
          }
        ]
      },
      "minecraft:behavior.float": {
        "priority": 0
      },
      "minecraft:behavior.nearest_attackable_target": {
        "priority": 1,
        "must_see": true,
        "reselect_targets": true,
        "within_radius": 25.0,
        "must_see_forget_duration": 17.0,
        "entity_types": [
          {
            "filters": {
              "any_of": [
                {
                  "test": "is_family",
                  "subject": "other",
                  "value": "player"
                }
              ]
            },
            "max_dist": 35
          }
        ]
      },
      "minecraft:behavior.melee_box_attack": {
        "priority": 2
      },
      "minecraft:behavior.random_stroll": {
        "priority": 3,
        "speed_multiplier": 1
      },
      "minecraft:behavior.look_at_player": {
        "priority": 4,
        "look_distance": 6.0,
        "probability": 0.02
      },
      "minecraft:behavior.random_look_around": {
        "priority": 5
      }
    },
    "events": {
      "sao:health_bar_show": {
        "set_property": {
          "sao:health_bar": true
        },
        "add": {
          "component_groups": [
            "sao:health_bar_timer"
          ]
        }
      },
      "sao:health_bar_hide": {
        "set_property": {
          "sao:health_bar": false
        },
        "remove": {
          "component_groups": [
            "sao:health_bar_timer"
          ]
        }
      }
    }
  }
}