{
  "format_version": "1.21.90",
  "minecraft:entity": {
    "description": {
      "identifier": "sao:tooltip",
      "is_spawnable": false,
      "is_summonable": true
    },
    "component_groups": {
      "sao:damage_indicator": {
        "minecraft:despawn": {
          "filters": {
            "any_of": [
              {
                "test": "inactivity_timer",
                "value": 1
              }
            ]
          }
        }
      },
      "sao:display_name": {
        "minecraft:despawn": {
          "filters": {
            "any_of": [
              {
                "test": "inactivity_timer",
                "value": 2
              }
            ]
          }
        }
      }
    },
    "components": {
      "minecraft:type_family": {
        "family": [
          "inanimate"
        ]
      },
      "minecraft:collision_box": {
        "width": 0,
        "height": 0
      },
      "minecraft:scale": {
        "value": 0
      },
      "minecraft:damage_sensor": {
        "triggers": {
          "deals_damage": "no"
        }
      },
      "minecraft:physics": {
        "has_gravity": false,
        "has_collision": false
      },
      "minecraft:pushable": {
        "is_pushable": false,
        "is_pushable_by_piston": false
      },
      "minecraft:nameable": {
        "always_show": true
      },
      "minecraft:despawn": {
        "filters": {
          "any_of": [
            {
              "test": "distance_to_nearest_player",
              "operator": ">",
              "value": 8
            }
          ]
        }
      },
      "minecraft:conditional_bandwidth_optimization": {
        "default_values": {
          "max_optimized_distance": 128,
          "max_dropped_ticks": 20,
          "use_motion_prediction_hints": true
        }
      },
      "minecraft:fire_immune": {}
    },
    "events": {
      "sao:damage_indicator": {
        "add": {
          "component_groups": [
            "sao:damage_indicator"
          ]
        }
      },
      "sao:display_name": {
        "add": {
          "component_groups": [
            "sao:display_name"
          ]
        }
      }
    }
  }
}