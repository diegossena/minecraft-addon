{
  "format_version": "1.21.90",
  "minecraft:entity": {
    "description": {
      "identifier": "sao:ragout_rabbit",
      "runtime_identifier": "minecraft:rabbit",
      "spawn_category": "creature",
      "is_spawnable": true,
      "is_summonable": true,
      "is_experimental": false,
      "properties": {
        "sao:health_bar": {
          "type": "bool",
          "client_sync": true,
          "default": "q.had_component_group('sao:health_bar_timer')"
        },
        "sao:exp": {
          "type": "int",
          "range": [
            0,
            12
          ],
          "default": 12
        },
        "sao:col": {
          "type": "int",
          "range": [
            10,
            30
          ],
          "default": 30
        }
      }
    },
    "component_groups": {
      "sao:health_bar_timer": {
        "minecraft:timer": {
          "time": 5,
          "looping": false,
          "time_down_event": {
            "event": "sao:health_bar_hide"
          }
        }
      }
    },
    "components": {
      "minecraft:health": {
        "value": 120,
        "max": 120
      },
      "minecraft:movement": {
        "value": 0.4
      },
      "minecraft:loot": {
        "table": "loot_tables/ragout_rabbit.json"
      },
      "minecraft:experience_reward": {
        "on_death": "query.last_hit_by_player ? q.property('sao:exp') : 0"
      },
      "minecraft:scale": {
        "value": 0.6
      },
      "minecraft:collision_box": {
        "width": 0.67,
        "height": 0.67
      },
      "minecraft:type_family": {
        "family": [
          "rabbit",
          "mob",
          "sao"
        ]
      },
      "minecraft:is_hidden_when_invisible": {},
      "minecraft:breathable": {
        "total_supply": 15,
        "suffocate_time": 0
      },
      "minecraft:nameable": {},
      "minecraft:hurt_on_condition": {
        "damage_conditions": [
          {
            "filters": {
              "test": "in_lava",
              "subject": "self",
              "operator": "==",
              "value": true
            },
            "cause": "lava",
            "damage_per_tick": 4
          }
        ]
      },
      "minecraft:navigation.walk": {
        "can_path_over_water": true,
        "avoid_water": true,
        "avoid_damage_blocks": true
      },
      "minecraft:movement.skip": {},
      "minecraft:jump.dynamic": {},
      "minecraft:can_climb": {},
      "minecraft:balloonable": {
        "mass": 0.4
      },
      "minecraft:despawn": {
        "despawn_from_distance": {}
      },
      "minecraft:physics": {},
      "minecraft:pushable": {
        "is_pushable": true,
        "is_pushable_by_piston": true
      },
      "minecraft:conditional_bandwidth_optimization": {},
      "minecraft:block_climber": {},
      "minecraft:behavior.float": {
        "priority": 0
      },
      "minecraft:behavior.panic": {
        "priority": 1,
        "speed_multiplier": 1.25
      },
      "minecraft:behavior.avoid_mob_type": {
        "priority": 2,
        "entity_types": [
          {
            "filters": {
              "test": "is_family",
              "subject": "other",
              "value": "player"
            },
            "max_dist": 16,
            "max_flee": 20,
            "sprint_speed_multiplier": 1.5
          }
        ]
      },
      "minecraft:behavior.random_stroll": {
        "priority": 3
      },
      "minecraft:behavior.random_look_around": {
        "priority": 4
      },
      "minecraft:behavior.look_at_player": {
        "priority": 5
      }
    },
    "events": {
      "sao:health_bar_show": {
        "set_property": {
          "sao:health_bar": true
        },
        "add": {
          "component_groups": [
            "sao:health_bar_timer"
          ]
        }
      },
      "sao:health_bar_hide": {
        "set_property": {
          "sao:health_bar": false
        },
        "remove": {
          "component_groups": [
            "sao:health_bar_timer"
          ]
        }
      }
    }
  }
}